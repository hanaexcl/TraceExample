<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup>
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>

  <system.diagnostics>
    <trace autoflush="true" >
      <listeners>
        <add name="testLog"
             type="System.Diagnostics.TextWriterTraceListener"
             initializeData="testLog.log">
        </add>
      </listeners>
    </trace>
  </system.diagnostics>
</configuration>